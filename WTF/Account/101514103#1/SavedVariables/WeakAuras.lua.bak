["custom"] = "function aura_env.GetLevel8DelvesDoneCount()\n    -- Getting \"world\" activities (so, delves)\n    local worldActivities = C_WeeklyRewards.GetActivities(Enum.WeeklyRewardChestThresholdType.World)\n    \n    -- Get the first activity\n    for i=2,1,-1 do \n        local activity = worldActivities[i]\n        \n        -- \"progress\" is the number of delves done, \"level\" the maximum level of that delve\n        if activity ~= nil and activity.level >= 8 then\n            return activity.progress\n        end\n    end\n    \n    -- if \"level\" is not 8 there's no way to find out.\n    return \"?\"\nend",
