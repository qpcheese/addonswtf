["custom"] = "function(e, ...)\n    if e == \"WQR_TIMER_TICK\" then\n        return true\n    end\n    \n    if e == \"WQR_ENABLE\" then\n        aura_env.saved.enabled = true\n        aura_env.printEnabled()\n        return true\n    elseif e == \"WQR_DISABLE\" then\n        aura_env.saved.enabled = false\n        aura_env.printEnabled()\n        return\n    elseif e == \"WQR_TOGGLE\" then\n        aura_env.saved.enabled = not(aura_env.saved.enabled)\n        aura_env.printEnabled()\n        return true\n    end\n    \n    if aura_env.saved.enabled then\n        if e == \"WQR_REMOVE_WAYPOINT\" then\n            local waypointID = tonumber(select(1, ...))\n            if waypointID then\n                aura_env.RemoveWaypointByQuestID(waypointID)\n            end\n        elseif e == \"WQR_REMOVE_ALL_WAYPOINTS\" then\n            aura_env.RemoveAllWaypoints()\n        elseif e == \"WQR_RESET_CACHE\" then\n            aura_env.ClearCache()\n        end\n    end\nend",
