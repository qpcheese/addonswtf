["custom"] = "function(e, w)\n    if (e == \"STATUS\" or e == \"UNIT_POWER_UPDATE\") and false then\n        aura_env.vigor = UnitPower(\"player\", Enum.PowerType.AlternateMount)\n        if aura_env.vigor == UnitPowerMax(\"player\", Enum.PowerType.AlternateMount) then\n            aura_env.charge = 0\n        end\n    elseif e == \"UPDATE_UI_WIDGET\" and w and false then\n        if w.widgetSetID == 283 then\n            local widgetInfo = C_UIWidgetManager.GetFillUpFramesWidgetVisualizationInfo(w.widgetID)\n            \n            if widgetInfo then\n                aura_env.charge = widgetInfo.fillValue / widgetInfo.fillMax\n            end \n        end\n    elseif e == \"FRAME_UPDATE\" or e == \"SPELL_UPDATE_COOLDOWN\" or e == \"STATUS\" then\n        local chargeInfo = C_Spell.GetSpellCharges(372610)\n        aura_env.vigor = chargeInfo.currentCharges\n        if aura_env.vigor == chargeInfo.maxCharges then\n            aura_env.charge = 0\n        else\n            aura_env.charge = (GetTime() - chargeInfo.cooldownStartTime) / chargeInfo.cooldownDuration\n        end\n        \n    end\n    return true\nend",
