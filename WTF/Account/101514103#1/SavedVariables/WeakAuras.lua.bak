["init"] = {
["custom"] = "function aura_env.InMountManiaArena()\n    local map = C_Map.GetBestMapForUnit(\"player\")\n    local position = map and C_Map.GetPlayerMapPosition(map, \"player\")\n    if not position then\n        return\n    end\n    local x, y = position:GetXY()\n    local a = {\n        0.6274, 0.5148,\n        0.6288, 0.5194,\n        0.6330, 0.5176,\n        0.6320, 0.5128,\n    }\n    local d =\n    abs(\n        abs(a[1]*(a[4]-a[6])+a[3]*(a[6]-a[2])+a[5]*(a[2]-a[4]))/2 +\n        abs(a[1]*(a[8]-a[6])+a[7]*(a[6]-a[2])+a[5]*(a[2]-a[8]))/2 -\n        (\n            abs(x*(a[2]-a[4])+a[1]*(a[4]-y)+a[3]*(y-a[2]))/2 +\n            abs(x*(a[4]-a[6])+a[3]*(a[6]-y)+a[5]*(y-a[4]))/2 +\n            abs(x*(a[6]-a[8])+a[5]*(a[8]-y)+a[7]*(y-a[6]))/2 +\n            abs(x*(a[8]-a[2])+a[7]*(a[2]-y)+a[1]*(y-a[8]))/2\n        )\n    )\n    if d + 1 ~= 1 then\n        return\n    end\n    return true\nend",
