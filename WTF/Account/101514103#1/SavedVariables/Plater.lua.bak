 0 and interruptReadyInTime then\n            return \"showReadyInTime\"\n        elseif interruptCD > 0 then\n            return \"showNotReady\"\n        elseif (castBar.channeling) then\n            return \"noshow\"\n        else        \n            return \"noshow\"\n        end\n    end\n    \n    function modTable.UpdateCast (unitId, unitFrame)\n        local castBar = unitFrame.castBar\n        local castBarHeight = castBar:GetHeight()\n        local castBarWidth = castBar:GetWidth()\n        \n        castBar.KickIcon:SetSize (castBarHeight, castBarHeight)\n        FormatCastName(unitId, castBar, castBarWidth)\n        Plater.SetAnchor(castBar.Text, {side = spellNameAnchorSide, x = spellNameXOffset, y = spellNameYOffset})\n        Plater.SetAnchor(unitFrame.castBar.KickIcon, iconAnchor)\n        castBar.KickIcon:Hide()\n        if (not Plater.IsInCombat()) then --not in combat\n            return\n        elseif (unitFrame.IsSelf) then --personal bar\n            return\n        end\n        \n        local iconStatus = GetInterruptStateOfCast (castBar)\n        \n        if (iconStatus == \"showReadyInTime\" or iconStatus == \"showNotReady\") then\n            if (modTable.config.anchor == spellNameAnchorSide) then\n                if (modTable.config.anchor == 10 or modTable.config.anchor == 3 or modTable.config.anchor == 1 or modTable.config.anchor == 6) then\n                    local truncatedCastBarWidth = castBarWidth - castBarHeight - spellNameXOffset - modTable.config.xOffset\n                    FormatCastName(unitId, castBar, truncatedCastBarWidth)\n                    Plater.SetAnchor(castBar.Text, {side = spellNameAnchorSide, x = castBarHeight + spellNameXOffset + modTable.config.xOffset, y = spellNameYOffset})\n                elseif (modTable.config.anchor == 11 or modTable.config.anchor == 5 or modTable.config.anchor == 7 or modTable.config.anchor == 2) then\n                    local truncatedCastBarWidth = castBarWidth - castBarHeight + spellNameXOffset + modTable.config.xOffset\n                    FormatCastName(unitId, castBar, truncatedCastBarWidth)\n                    Plater.SetAnchor(castBar.Text, {side = spellNameAnchorSide, x = -castBarHeight + spellNameXOffset + modTable.config.xOffset, y = spellNameYOffset})\n                end\n            end\n        end\n        \n        if (iconStatus == \"showReadyInTime\") then\n            castBar.KickIcon:SetVertexColor (unpack (readyInTimeColor))\n            castBar.KickIcon:SetTexture (readyInTimeTexture)\n            castBar.KickIcon:Show()\n        elseif(iconStatus == \"showNotReady\") then\n            castBar.KickIcon:SetVertexColor (unpack (notReadyColor))\n            castBar.KickIcon:SetTexture (notReadyTexture)\n            castBar.KickIcon:Show()\n        end\n    end\n    \n    function modTable.ConstructIcon(unitFrame)\n        unitFrame.castBar.KickIcon = unitFrame.castBar.KickIcon or unitFrame.castBar:CreateTexture (nil, \"overlay\")\n        Plater.SetAnchor(unitFrame.castBar.KickIcon, iconAnchor)\n        \n        unitFrame.castBar.KickIcon.Animate =  unitFrame.castBar.KickIcon.Animate or Plater:CreateAnimationHub (unitFrame.castBar.KickIcon, \n            function()\n            end,\n            function()\n            end\n        )\n        \n        -- optional spin animation component        \n        unitFrame.castBar.KickIcon.Animate.scaleUp = unitFrame.castBar.KickIcon.Animate.scaleUp or Plater:CreateAnimation (unitFrame.castBar.KickIcon.Animate, \"Scale\", 1, 0.15, 0, 0, 2, 2)\n        \n        unitFrame.castBar.KickIcon.Animate.wait = unitFrame.castBar.KickIcon.Animate.wait or Plater:CreateAnimation (unitFrame.castBar.KickIcon.Animate, \"Rotation\", 2, 0.3, 0)\n        \n        unitFrame.castBar.KickIcon.Animate.scaleDown = unitFrame.castBar.KickIcon.Animate.scaleDown or Plater:CreateAnimation (unitFrame.castBar.KickIcon.Animate, \"Scale\", 3, 0.1, 1, 1, 0.5, 0.5)\n        \n        --unitFrame.castBar.KickIcon.Animate.spin = unitFrame.castBar.KickIcon.Animate.spin or Plater:CreateAnimation (unitFrame.castBar.KickIcon.Animate, \"Rotation\", 3, 0.2, -360)\n    end\n    \nend",
