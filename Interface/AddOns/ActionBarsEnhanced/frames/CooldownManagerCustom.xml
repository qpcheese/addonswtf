<Ui xmlns="http://www.blizzard.com/wow/ui/">
    <Frame name="ABE_CDMCustomItemBaseTemplate" inherits="CallbackRegistrantTemplate" virtual="true" enableMouseClicks="false" dontSavePosition="true">
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
			<OnShow method="OnShow"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>
    <Frame name="ABE_CDMCustomItemTemplate" inherits="ABE_CDMCustomItemBaseTemplate" mixin="ABE_CDMCustomItemMixin" virtual="true" hidden="false" dontSavePosition="true">
		<Size x="40" y="40"/>
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="Icon" setAllPoints="true"/>
				<MaskTexture parentKey="IconMask" atlas="UI-HUD-CoolDownManager-Mask" setAllPoints="true">
					<MaskedTextures>
						<MaskedTexture childKey="Icon"/>
					</MaskedTextures>
				</MaskTexture>
			</Layer>
			
		</Layers>
		<Frames>
			<Frame parentKey="ProcGlow" mixin="ABE_CDMCustomItemProcGlow" hidden="false" dontSavePosition="true">
				<Size x="40" y="40"/>
				<Anchors>
					<Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER" x="-2" y="0" />
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="ProcStartFlipbook" atlas="UI-HUD-ActionBar-Proc-Start-Flipbook" alpha="0">
							<Size x="150" y="150"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
						<Texture parentKey="ProcLoopFlipbook" atlas="UI-HUD-ActionBar-Proc-Loop-Flipbook" setAllPoints="true" alpha="0"/>
						<Texture parentKey="ProcAltGlow" atlas="UI-HUD-RotationHelper-ProcAltGlow" useAtlasSize="true" hidden="true">
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Animations>
					<AnimationGroup parentKey="ProcLoop" looping="REPEAT">
						<Alpha childKey="ProcLoopFlipbook" duration=".001" order="0" fromAlpha="1" toAlpha="1"/>
						<FlipBook parentKey="FlipAnim" childKey="ProcLoopFlipbook" duration="1" order="0" flipBookRows="6" flipBookColumns="5" flipBookFrames="30" flipBookFrameWidth="0" flipBookFrameHeight="0"/>
					</AnimationGroup>
						
					<AnimationGroup parentKey="ProcStartAnim" setToFinalAlpha="true">
						<Alpha childKey="ProcStartFlipbook" duration=".001" order="0" fromAlpha="1" toAlpha="1"/>
						<FlipBook childKey="ProcStartFlipbook" duration="0.7" order="1" flipBookRows="6" flipBookColumns="5" flipBookFrames="30" flipBookFrameWidth="0" flipBookFrameHeight="0"/>
						<Alpha childKey="ProcStartFlipbook" duration=".001" order="2" fromAlpha="1" toAlpha="0"/>
					</AnimationGroup>
				</Animations>
				<Scripts>
					<OnLoad method="OnLoad"/>
					<OnHide method="OnHide"/>
				</Scripts>
			</Frame>
			<Cooldown parentKey="Cooldown" reverse="true" setAllPoints="true">
				<SwipeTexture file="Interface\HUD\UI-HUD-CoolDownManager-Icon-Swipe">
					<Color r="0" g="0" b="0" a="0.7"/>
				</SwipeTexture>
				<EdgeTexture file="Interface\Cooldown\UI-HUD-ActionBar-SecondaryCooldown"/>
			</Cooldown>
            <Cooldown parentKey="AuraCooldown" reverse="false" setAllPoints="true">
				<SwipeTexture file="Interface\HUD\UI-HUD-CoolDownManager-Icon-Swipe">
					<Color r="1" g="0.95" b="0.57" a="0.7"/>
				</SwipeTexture>
				<EdgeTexture file="Interface\Cooldown\UI-HUD-ActionBar-SecondaryCooldown"/>
			</Cooldown>
			<Frame parentKey="Applications" setAllPoints="true">
				<Layers>
					<Layer level="OVERLAY">
						<FontString parentKey="Applications" inherits="NumberFontNormal">
							<Anchors>
								<Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
	</Frame>
	<Frame name="ABE_CDMCustomBarStagePipTemplate" useParentLevel="true" virtual="true">
		<Size x="2" y="10"/>
		<Layers>
			<Layer level="ARTWORK" textureSubLevel="2">
				<Texture parentKey="BasePip" file="Interface\Buttons\WHITE8x8" setAllPoints="true"/>
			</Layer>
		</Layers>
	</Frame>
	<Frame name="ABE_CDMCustomBarTemplate" inherits="ABE_CDMCustomItemBaseTemplate" mixin="ABE_CDMCustomBarMixin" virtual="true" hidden="false" dontSavePosition="true">
		<Size x="200" y="20"/>
		<Frames>
			<Frame parentKey="Icon">
				<Size x="20" y="20"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent" relativePoint="LEFT" x="0" y="0" />
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="Icon" setAllPoints="true"/>
						<MaskTexture parentKey="IconMask" atlas="UI-HUD-CoolDownManager-Mask" setAllPoints="true">
							<MaskedTextures>
								<MaskedTexture childKey="Icon"/>
							</MaskedTextures>
						</MaskTexture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture parentKey="IconOverlay" atlas="UI-HUD-CoolDownManager-IconOverlay">
							<Anchors>
								<Anchor point="TOPLEFT" x="-8" y="7"/>
								<Anchor point="BOTTOMRIGHT" x="8" y="-7"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Frames>
					<Cooldown parentKey="Cooldown" reverse="true" setAllPoints="true">
						<SwipeTexture file="Interface\HUD\UI-HUD-CoolDownManager-Icon-Swipe">
							<Color r="0" g="0" b="0" a="0.7"/>
						</SwipeTexture>
						<EdgeTexture file="Interface\Cooldown\UI-HUD-ActionBar-SecondaryCooldown"/>
					</Cooldown>
					<Cooldown parentKey="AuraCooldown" reverse="false" setAllPoints="true">
						<SwipeTexture file="Interface\HUD\UI-HUD-CoolDownManager-Icon-Swipe">
							<Color r="1" g="0.95" b="0.57" a="0.7"/>
						</SwipeTexture>
						<EdgeTexture file="Interface\Cooldown\UI-HUD-ActionBar-SecondaryCooldown"/>
					</Cooldown>
					<Frame parentKey="Applications" setAllPoints="true">
						<Layers>
							<Layer level="OVERLAY">
								<FontString parentKey="Applications" inherits="NumberFontNormal">
									<Anchors>
										<Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
									</Anchors>
								</FontString>
							</Layer>
						</Layers>
					</Frame>
				</Frames>
			</Frame>
			<StatusBar parentKey="Bar">
				<Size y="19"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="0" y="0"/>
					<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
				</Anchors>
				<BarTexture atlas="UI-HUD-CoolDownManager-Bar" setAllPoints="true">
					<Color r="1.0" g="0.5" b="0.25"/>
				</BarTexture>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="BarBG" atlas="UI-HUD-CoolDownManager-Bar-BG">
							<Anchors>
								<Anchor point="TOPLEFT" x="0" y="0"/>
								<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture parentKey="Pip" atlas="UI-HUD-CoolDownManager-Bar-Pip" useAtlasSize="true">
							<Size x="8" y="19"/>
						</Texture>
						<FontString parentKey="Name" inherits="NumberFontNormal" justifyH="LEFT" justifyV="MIDDLE">
							<Anchors>
								<Anchor point="LEFT" relativePoint="LEFT" x="2" y="0"/>
							</Anchors>
						</FontString>
						<FontString parentKey="Duration" inherits="NumberFontNormal" justifyH="LEFT">
							<Anchors>
								<Anchor point="RIGHT" x="-8"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</StatusBar>
		</Frames>
	</Frame>


    <Frame name="ABE_CDMCustomFrameTemplate" inherits="GridLayoutFrame, CallbackRegistrantTemplate" parent="UIParent" virtual="true" dontSavePosition="true" enableMouseClicks="true" >
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
			<OnEvent method="OnEvent"/>
			<OnUpdate method="OnUpdate"/>
		</Scripts>
	</Frame>
    
    <Frame name="ABE_CDMCustomFrameBase" inherits="ABE_CDMCustomFrameTemplate" virtual="true" clampedToScreen="true" dontSavePosition="true">
        <Frames>		
            <Frame parentKey="Container" inherits="GridLayoutFrame" >
                <KeyValues>
                    <KeyValue key="childXPadding" value="2" type="number"/>
                    <KeyValue key="childYPadding" value="2" type="number"/>
                    <KeyValue key="isHorizontal" value="true" type="boolean"/>
                    <KeyValue key="stride" value="1" type="number"/>
                    <KeyValue key="layoutFramesGoingRight" value="true" type="boolean"/>
                    <KeyValue key="layoutFramesGoingUp" value="false" type="boolean"/>
                    <KeyValue key="alwaysUpdateLayout" value="true" type="boolean"/>
                </KeyValues>

            </Frame>

            <Frame parentKey="ABESelection" mixin="ABE_CDMCustomFrameSelectionMixin" inherits="NineSliceCodeTemplate" setAllPoints="true" hidden="true" frameLevel="90" registerForDrag="LeftButton" enableMouse="true" dontSavePosition="true" virtual="true" alpha="0.3">
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent.Container" x="-4" y="4"/>
					<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Container" x="4" y="-4"/>
				</Anchors>
				<KeyValues>
					<KeyValue key="highlightTextureKit" value="editmode-actionbar-highlight" type="string"/>
					<KeyValue key="selectedTextureKit" value="editmode-actionbar-selected" type="string"/>
					<KeyValue key="ignoreInLayout" value="true" type="boolean"/>
				</KeyValues>
				<Layers>
					<Layer level="OVERLAY">
						<FontString parentKey="Label" inherits="GameFontHighlightLarge" mixin="ShrinkUntilTruncateFontStringMixin" setAllPoints="true" ignoreParentScale="true" hidden="true" text="Custom Frame"/>
					</Layer>
				</Layers>
				<Frames>
					<Frame parentKey="FrameGrid" hidden="false" alpha="0.7" ignoreParentAlpha="true" setAllPoints="true">
						<Layers>
							<Layer level="OVERLAY">
								<Texture parentKey="GridHorizRight" file="Interface\AddOns\ActionBarsEnhanced\assets\frames\selection_grid_horiz.png" vertTile="true" horizTile="true">
									<Anchors>
										<Anchor point="LEFT" relativeKey="$parent" relativePoint="CENTER"/>
										<Anchor point="RIGHT" relativeKey="$parent" relativePoint="RIGHT"/>
									</Anchors>
								</Texture>
								<Texture parentKey="GridHorizLeft" file="Interface\AddOns\ActionBarsEnhanced\assets\frames\selection_grid_horiz.png" vertTile="true" horizTile="true">
									<Anchors>
										<Anchor point="RIGHT" relativeKey="$parent" relativePoint="CENTER"/>
										<Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT"/>
									</Anchors>
								</Texture>
								<Texture parentKey="GridVertUp" file="Interface\AddOns\ActionBarsEnhanced\assets\frames\selection_grid_vert.png" vertTile="true" horizTile="true">
									<Anchors>
										<Anchor point="BOTTOM" relativeKey="$parent" relativePoint="CENTER"/>
										<Anchor point="TOP" relativeKey="$parent" relativePoint="TOP"/>
									</Anchors>
								</Texture>
								<Texture parentKey="GridVertDown" file="Interface\AddOns\ActionBarsEnhanced\assets\frames\selection_grid_vert.png" vertTile="true" horizTile="true">
									<Anchors>
										<Anchor point="TOP" relativeKey="$parent" relativePoint="CENTER"/>
										<Anchor point="BOTTOM" relativeKey="$parent" relativePoint="BOTTOM"/>
									</Anchors>
								</Texture>
							</Layer>
						</Layers>
					</Frame>
					<Frame parentKey="MouseOverHighlight" inherits="NineSliceCodeTemplate" setAllPoints="true" alpha="0.4" hidden="true" virtual="true">
						<Scripts>
							<OnLoad function="nop"/>
						</Scripts>
					</Frame>
				</Frames>
				<Scripts>
					<OnDragStart method="OnDragStart"/>
					<OnDragStop method="OnDragStop"/>
					<OnMouseDown method="OnMouseDown"/>
					<OnLoad method="OnLoad"/>
					<OnEnter method="OnEnter"/>
					<OnLeave method="OnLeave"/>
					<OnShow method="OnShow"/>
					<OnHide method="OnHide"/>
					<OnKeyDown method="OnKeyDown"/>
				</Scripts>
			</Frame>
        </Frames>

		<Animations>
			<AnimationGroup parentKey="PulseAnim" looping="BOUNCE">
				<Alpha childKey="ABESelection" fromAlpha=".1" toAlpha=".5" duration=".8"/>
			</AnimationGroup>
		</Animations>
	</Frame>
	<Frame name="ABE_CDMCustomFrame" mixin="ABE_CDMCustomFrameMixin" inherits="ABE_CDMCustomFrameBase" virtual="true">
		<KeyValues>
			<KeyValue key="itemTemplate" value="ABE_CDMCustomItemTemplate" type="string"/>
		</KeyValues>
	</Frame>
	<Frame name="ABE_CDMCustomBarFrame" mixin="ABE_CDMCustomBarFrameMixin" inherits="ABE_CDMCustomFrameBase" virtual="true">
		<KeyValues>
			<KeyValue key="itemTemplate" value="ABE_CDMCustomBarTemplate" type="string"/>
		</KeyValues>
	</Frame>
</Ui>