<Ui xmlns="http://www.blizzard.com/wow/ui/">

	<Frame name="ABE_BarsFrameTemplate" mixin="ABE_BarsFrameMixin" frameStrata="LOW" virtual="true" enableMouse="true" hidden="false">
        <Size x="200" y="740"/>
        <Anchors>
            <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER"/>
        </Anchors>
        <Frames>
            <Frame parentKey="NineSlice" inherits="NineSlicePanelTemplate" setAllPoints="true" hidden="false" frameLevel="90">
				<Anchors>
					<Anchor point="TOPLEFT" x="-8" y="8"/>
					<Anchor point="BOTTOMRIGHT" x="8" y="-8"/>
				</Anchors>
				<Layers>
                    <Layer level="BACKGROUND" textureSubLevel="-5">
                        <Texture parentKey="Bg" atlas="auctionhouse-background-sell-right" alpha="0.9">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="7" y="-7"/>
                                <Anchor point="BOTTOMRIGHT" x="-7" y="7"/>
                            </Anchors>
                        </Texture>
                    </Layer>
					<Layer level="BORDER">
						<Texture parentKey="TopLeftCorner" atlas="UI-Frame-DiamondMetal-CornerTopLeft" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="TOPLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="TopRightCorner" atlas="UI-Frame-DiamondMetal-CornerTopRight" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="TOPRIGHT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="BottomLeftCorner" atlas="UI-Frame-DiamondMetal-CornerBottomLeft" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="BOTTOMLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="BottomRightCorner" atlas="UI-Frame-DiamondMetal-CornerBottomRight" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="BOTTOMRIGHT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="TopEdge" atlas="_UI-Frame-DiamondMetal-EdgeTop" useAtlasSize="true" horizTile="true">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeKey="$parent.TopLeftCorner" relativePoint="TOPRIGHT"/>
                                <Anchor point="TOPRIGHT" relativeKey="$parent.TopRightCorner" relativePoint="TOPLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="BottomEdge" atlas="_UI-Frame-DiamondMetal-EdgeBottom" useAtlasSize="true" horizTile="true">
                            <Anchors>
                                <Anchor point="BOTTOMLEFT" relativeKey="$parent.BottomLeftCorner" relativePoint="BOTTOMRIGHT"/>
                                <Anchor point="BOTTOMRIGHT" relativeKey="$parent.BottomRightCorner" relativePoint="BOTTOMLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="LeftEdge" atlas="!UI-Frame-DiamondMetal-EdgeLeft" useAtlasSize="true" vertTile="true">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeKey="$parent.TopLeftCorner" relativePoint="BOTTOMLEFT"/>
                                <Anchor point="BOTTOMLEFT" relativeKey="$parent.BottomLeftCorner" relativePoint="TOPLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="RightEdge" atlas="!UI-Frame-DiamondMetal-EdgeRight" useAtlasSize="true" vertTile="true">
                            <Anchors>
                                <Anchor point="TOPRIGHT" relativeKey="$parent.TopRightCorner" relativePoint="BOTTOMRIGHT"/>
                                <Anchor point="BOTTOMRIGHT" relativeKey="$parent.BottomRightCorner" relativePoint="TOPRIGHT"/>
                            </Anchors>
                        </Texture>
					</Layer>
					<Layer level="ARTWORK" textureSubLevel="1">
						<Texture parentKey="RightBorderDecoration" atlas="UI-Frame-CypherChoice-CardParchment-Style3" useAtlasSize="false" desaturated="true">
							<Size x="70" y="130"/>
							<Color r="0.7" g="0.7" b="0.7"/>
							<Anchors>
								<Anchor point="CENTER" relativeKey="$parent.RightEdge" relativePoint="CENTER" x="0" y="0"/>
							</Anchors>
						</Texture>
						<MaskTexture parentKey="IconMask" atlas="SquareMask" hWrapMode="CLAMPTOBLACKADDITIVE" vWrapMode="CLAMPTOBLACKADDITIVE" useAtlasSize="false">
							<Size x="70" y="130"/>
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent" relativePoint="RIGHT" x="-6" y="0"/>
							</Anchors>
							<MaskedTextures>
								<MaskedTexture childKey="RightBorderDecoration"/>
							</MaskedTextures>
						</MaskTexture>
					</Layer>
                    <Layer level="OVERLAY">
                        <FontString parentKey="Title" inherits="GameFontNormal" text="Advanced">
							<Shadow x="1" y="-1">
								<Color r="0" g="0" b="0"/>
							</Shadow>
							<Anchors>
								<Anchor point="CENTER" relativeKey="$parent.RightBorderDecoration" relativePoint="RIGHT" x="-22" y="-5"/>
							</Anchors>
							<Color color="ARTIFACT_GOLD_COLOR"/>
						</FontString>
                    </Layer>
				</Layers>
			</Frame>
			<Button parentKey="ABE_BarsFrameButton" mixin="ABE_BarsFrameMixin" virtual="true" frameLevel="95">
				<Size x="20" y="112"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.NineSlice.RightBorderDecoration" relativePoint="RIGHT" x="-8" y="0"/>
				</Anchors>
				<Scripts>
					<OnLoad method="OnLoad"/>
					<OnClick method="OnClick"/>
					<OnHide method="OnHide"/>
				</Scripts>
				<HighlightTexture parentKey="HighlightTexture" file="Interface/addons/ActionBarsEnhanced/assets/button_highlight.png" alphaMode="ADD"/>
			</Button>
        </Frames>
    </Frame>
	<Frame name="ABE_BarsHighlightTemplate" mixin="ABE_BarsFrameMixin" virtual="true" hidden="true">
		<Frames>
			<Frame parentKey="NineSlice" inherits="NineSlicePanelTemplate" setAllPoints="true" hidden="false" frameLevel="90">
				<Anchors>
					<Anchor point="TOPLEFT" x="-8" y="8"/>
					<Anchor point="BOTTOMRIGHT" x="8" y="-8"/>
				</Anchors>
				<Layers>
                    <Layer level="BACKGROUND" textureSubLevel="-5">
                        <Texture parentKey="Bg" atlas="editmode-actionbar-highlight-nineslice-center" alpha="0.5">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="7" y="-7"/>
                                <Anchor point="BOTTOMRIGHT" x="-7" y="7"/>
                            </Anchors>
                        </Texture>
                    </Layer>
					<Layer level="BORDER">
						<Texture parentKey="TopLeftCorner" atlas="editmode-actionbar-highlight-nineslice-corner" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="TOPLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="TopRightCorner" atlas="editmode-actionbar-highlight-nineslice-corner" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="TOPRIGHT"/>
                            </Anchors>
							<TexCoords left="1" right="0" top="0" bottom="1"/>
                        </Texture>
                        <Texture parentKey="BottomLeftCorner" atlas="editmode-actionbar-highlight-nineslice-corner" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="BOTTOMLEFT"/>
                            </Anchors>
							<TexCoords left="0" right="1" top="1" bottom="0"/>
                        </Texture>
                        <Texture parentKey="BottomRightCorner" atlas="editmode-actionbar-highlight-nineslice-corner" useAtlasSize="true">
                            <Anchors>
                                <Anchor point="BOTTOMRIGHT"/>
                            </Anchors>
							<TexCoords left="1" right="0" top="1" bottom="0"/>
                        </Texture>
                        <Texture parentKey="TopEdge" atlas="_editmode-actionbar-highlight-nineslice-edgetop" useAtlasSize="true" horizTile="true">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeKey="$parent.TopLeftCorner" relativePoint="TOPRIGHT"/>
                                <Anchor point="TOPRIGHT" relativeKey="$parent.TopRightCorner" relativePoint="TOPLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="BottomEdge" atlas="_editmode-actionbar-highlight-nineslice-edgebottom" useAtlasSize="true" horizTile="true">
                            <Anchors>
                                <Anchor point="BOTTOMLEFT" relativeKey="$parent.BottomLeftCorner" relativePoint="BOTTOMRIGHT"/>
                                <Anchor point="BOTTOMRIGHT" relativeKey="$parent.BottomRightCorner" relativePoint="BOTTOMLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="LeftEdge" atlas="!editmode-actionbar-highlight-nineslice-edgeleft" useAtlasSize="true" vertTile="true">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeKey="$parent.TopLeftCorner" relativePoint="BOTTOMLEFT"/>
                                <Anchor point="BOTTOMLEFT" relativeKey="$parent.BottomLeftCorner" relativePoint="TOPLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="RightEdge" atlas="!editmode-actionbar-highlight-nineslice-edgeright" useAtlasSize="true" vertTile="true">
                            <Anchors>
                                <Anchor point="TOPRIGHT" relativeKey="$parent.TopRightCorner" relativePoint="BOTTOMRIGHT"/>
                                <Anchor point="BOTTOMRIGHT" relativeKey="$parent.BottomRightCorner" relativePoint="TOPRIGHT"/>
                            </Anchors>
                        </Texture>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		
		<Animations>
			<AnimationGroup parentKey="PulseAnim" looping="REPEAT">
				<Alpha childKey="NineSlice" fromAlpha=".3" toAlpha=".5" duration=".2" order="1"/>
				<Alpha childKey="NineSlice" fromAlpha=".5" toAlpha=".3" startDelay=".2" duration=".4" order="1"/>
			</AnimationGroup>
		</Animations>
	</Frame>
	
    <Frame name="ABE_BarsListSpacerTemplate" virtual="true">
		<Size y="18"/>
	</Frame>

	<Frame name="ABE_BarsListTemplate" mixin="ABE_BarsListMixin" virtual="true">
		<Frames>
			<Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
				<Anchors>
					<Anchor point="TOPLEFT" x="0" y="0"/>
					<Anchor point="BOTTOMRIGHT"/>
				</Anchors>
			</Frame>
			<EventFrame parentKey="ScrollBar" inherits="MinimalScrollBar">
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent.ScrollBox" relativePoint="TOPRIGHT" x="0" y="2"/>
					<Anchor point="BOTTOMLEFT" relativeKey="$parent.ScrollBox" relativePoint="BOTTOMRIGHT" x="-1" y="7"/>
				</Anchors>
			</EventFrame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
		</Scripts>
	</Frame>

	<Frame name="ABE_BarsListHeaderTemplate" mixin="ABE_BarsListHeaderMixin" virtual="true">
		<Size x="190" y="31"/>
