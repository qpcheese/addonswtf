	<Button name="ABE_BarsListButtonTemplate" mixin="ABE_BarsButtonMixin" virtual="true">
		<Size x="175" y="20"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="Texture" atlas="plunderstorm-spectate-background" alphaMode="ADD" hidden="true">
					<Size x="195" y="20"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontNormal" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT" x="36" y="1"/>
						<Anchor point="BOTTOMRIGHT" y="1"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="NewFeature" inherits="NewFeatureLabelTemplate" frameStrata="HIGH" scale=".8" frameLevel="100" hidden="true">
				<Size x="1" y="1"/>
				<Anchors>
					<Anchor point="BOTTOMRIGHT" x="-4" y="-10" relativeKey="$parent.Label" relativePoint="LEFT"/>
				</Anchors>
			</Frame>
			<Button parentKey="Reset" hidden="false">
				<Size x="11" y="11"/>
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" x="5" y="0"/>
				</Anchors>
				<NormalTexture atlas="common-search-clearbutton" alpha="0.8"/>
				<HighlightTexture atlas="common-search-clearbutton" alphaMode="ADD" alpha="0.7"/>
				<PushedTexture atlas="common-search-clearbutton" alpha="0.8">
					<Color r="0.5" g="0.5" b="0.5"/>	
				</PushedTexture>
			</Button>
			<Button parentKey="Copy" hidden="true">
				<Size x="18" y="18"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent" relativePoint="RIGHT" x="-2" y="0"/>
				</Anchors>
				<NormalTexture file="Interface/addons/ActionBarsEnhanced/assets/copy_icon.png" alpha="0.3"/>
				<HighlightTexture file="Interface/addons/ActionBarsEnhanced/assets/copy_icon.png" alphaMode="ADD" alpha="0.2"/>
				<PushedTexture file="Interface/addons/ActionBarsEnhanced/assets/copy_icon.png" alpha="0.3">
					<Color r="0.5" g="0.5" b="0.5"/>	
				</PushedTexture>
			</Button>
			<Button parentKey="Paste" hidden="true">
				<Size x="18" y="18"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent" relativePoint="RIGHT" x="-2" y="0"/>
				</Anchors>
				<NormalTexture file="Interface/addons/ActionBarsEnhanced/assets/paste_icon.png" alpha="0.3"/>
				<HighlightTexture file="Interface/addons/ActionBarsEnhanced/assets/paste_icon.png" alphaMode="ADD" alpha="0.2"/>
				<PushedTexture file="Interface/addons/ActionBarsEnhanced/assets/paste_icon.png" alpha="0.3">
					<Color r="0.5" g="0.5" b="0.5"/>	
				</PushedTexture>
			</Button>
		</Frames>
		<HighlightTexture atlas="plunderstorm-spectate-background" alphaMode="ADD" desaturated="true" alpha="0.4">
			<Size x="195" y="20"/>
		</HighlightTexture>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnHide method="OnHide"/>
			<OnShow method="OnShow"/>
		</Scripts>
	</Button>
</Ui>