			<Layer level="ARTWORK">
				<Texture parentKey="Background" atlas="house-upgrade-reward-track-bg" useAtlasSize="false">
					<Size x="190" y="31"/>
					<Anchors>
						<Anchor point="TOPLEFT" y="5"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<FontString parentKey="Label" inherits="GameFontHighlightMedium" justifyH="LEFT">
					<Shadow x="1" y="-1">
						<Color r="0" g="0" b="0"/>
					</Shadow>
					<Color color="PAPER_FRAME_EXPANDED_COLOR"/>
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.Background" x="30" y="0"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
