		<Frames>
			<Frame parentKey="ProcGlow" hidden="false">
				<Size x="65" y="65"/>
				<Anchors>
					<Anchor point="CENTER" x="0" y="0" relativeKey="$parent" relativePoint="CENTER"/>
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="ProcStartFlipbook" atlas="UI-HUD-ActionBar-Proc-Start-Flipbook" alpha="0">
							<Size x="150" y="150"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
						<Texture parentKey="ProcLoopFlipbook" atlas="UI-HUD-ActionBar-Proc-Loop-Flipbook" setAllPoints="true" alpha="0"/>
