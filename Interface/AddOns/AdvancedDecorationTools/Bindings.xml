<Bindings>
    <Binding name="ADT_TOGGLE_HISTORY" header="ADT">
        ADT_ToggleHistory();
    </Binding>
    <Binding name="ADT_TEMP_STORE">
        ADT_Temp_StoreSelected();
    </Binding>
    <Binding name="ADT_TEMP_STORE_COPY">
        ADT_Temp_StoreSelectedCopy();
    </Binding>
    <Binding name="ADT_TEMP_RECALL">
        ADT_Temp_RecallTop();
    </Binding>
    <Binding name="ADT_ROTATE_CCW_90">
        ADT_Rotate_CCW_90();
    </Binding>
    <Binding name="ADT_ROTATE_CW_90">
        ADT_Rotate_CW_90();
    </Binding>
    <!-- 专家模式：快速选轴 -->
    <Binding name="ADT_AXIS_X" header="ADT_EXPERT">
        ADT_Axis_X();
    </Binding>
    <Binding name="ADT_AXIS_Y">
        ADT_Axis_Y();
    </Binding>
    <Binding name="ADT_AXIS_Z">
        ADT_Axis_Z();
    </Binding>
</Bindings>
